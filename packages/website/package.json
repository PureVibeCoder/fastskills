{
  "name": "@fastskills/website",
  "version": "1.0.0",
  "type": "module",
  "scripts": {
    "dev": "node scripts/inject-skill-content.mjs && astro dev",
    "build": "node scripts/inject-skill-content.mjs && astro build",
    "preview": "astro preview",
    "astro": "astro",
    "typecheck": "tsc --noEmit",
    "test": "vitest run",
    "test:watch": "vitest",
    "test:ui": "vitest --ui",
    "lint": "eslint src/",
    "lint:fix": "eslint src/ --fix",
    "format": "prettier --write src/",
    "format:check": "prettier --check src/"
  },
  "dependencies": {
    "@astrojs/cloudflare": "^12.6.12",
    "@astrojs/node": "^9.5.1",
    "astro": "^5.16.6",
    "jszip": "^3.10.1"
  },
  "devDependencies": {
    "@eslint/js": "^9.39.2",
    "@types/jszip": "^3.4.0",
    "@typescript-eslint/eslint-plugin": "^8.51.0",
    "@typescript-eslint/parser": "^8.51.0",
    "@vitest/ui": "^4.0.16",
    "eslint": "^9.39.2",
    "eslint-config-prettier": "^10.1.8",
    "eslint-plugin-astro": "^1.5.0",
    "globals": "^17.0.0",
    "happy-dom": "^20.0.11",
    "prettier": "^3.7.4",
    "prettier-plugin-astro": "^0.14.1",
    "typescript": "^5.0.0",
    "vitest": "^4.0.16"
  }
}
